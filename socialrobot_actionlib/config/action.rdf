    <!-- http://www.semanticweb.org/socialrobot#approach_arm -->

    <owl:Class rdf:about="http://www.semanticweb.org/socialrobot#approach_arm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pddl#action"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/socialrobot#hold_object"/>
        <pddl:effect>
            <rdf:Description>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#nearby"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param1>robotPart</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param2>targetObject</pddl:param2>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#not"/>
                        <pddl:predicate>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/pddl#locatedAt"/>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                        <pddl:param1>robotPart</pddl:param1>
                                    </rdf:Description>
                                </pddl:parameter>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
                                        <pddl:param2>initPosition</pddl:param2>
                                    </rdf:Description>
                                </pddl:parameter>
                            </rdf:Description>
                        </pddl:predicate>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#locatedAt"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param1>robotPart</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
                                <pddl:param2>nearPosition</pddl:param2>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
            </rdf:Description>
        </pddl:effect>
        <pddl:parameters>
            <rdf:Description>
                <pddl:parameter>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                        <pddl:param1>robotPart</pddl:param1>
                    </rdf:Description>
                </pddl:parameter>
                <pddl:parameter>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                        <pddl:param2>targetObject</pddl:param2>
                    </rdf:Description>
                </pddl:parameter>
                <pddl:parameter>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
