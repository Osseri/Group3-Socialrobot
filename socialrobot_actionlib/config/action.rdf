                                <pddl:param1>nearPosition</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:parameter>
                            <rdf:Description>
                                <pddl:param2>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Nearby"/>
                                    </rdf:Description>
                                </pddl:param2>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#locatedAt"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param1>robotPart</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
                                <pddl:param2>initPosition</pddl:param2>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#forall"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param1>objects</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:predicate>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/pddl#not"/>
                                <pddl:predicate>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#obstruct"/>
                                        <pddl:parameter>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                                <pddl:param1>robotPart</pddl:param1>
