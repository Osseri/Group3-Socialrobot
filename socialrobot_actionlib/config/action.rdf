                    </rdf:Description>
                </pddl:parameter>
            </rdf:Description>
        </pddl:parameters>
        <pddl:precondition>
            <rdf:Description>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#not"/>
                        <pddl:predicate>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#graspedBy"/>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                        <pddl:param1>robotPart</pddl:param1>
                                    </rdf:Description>
                                </pddl:parameter>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                        <pddl:param2>targetObject</pddl:param2>
                                    </rdf:Description>
                                </pddl:parameter>
                            </rdf:Description>
                        </pddl:predicate>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#not"/>
                        <pddl:predicate>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/pddl#equal"/>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
                                        <pddl:param1>initPosition</pddl:param1>
                                    </rdf:Description>
                                </pddl:parameter>
                                <pddl:parameter>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Position"/>
                                        <pddl:param2>nearPosition</pddl:param2>
                                    </rdf:Description>
                                </pddl:parameter>
                            </rdf:Description>
                        </pddl:predicate>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#openedHand"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <pddl:param1>robotPart</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#type"/>
                        <pddl:parameter>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Object"/>
                                <pddl:param1>robotPart</pddl:param1>
                            </rdf:Description>
                        </pddl:parameter>
                        <pddl:parameter>
                            <rdf:Description>
                                <pddl:param2>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.semanticweb.org/socialrobot#Gripper"/>
                                    </rdf:Description>
                                </pddl:param2>
                            </rdf:Description>
                        </pddl:parameter>
                    </rdf:Description>
                </pddl:predicate>
                <pddl:predicate>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.semanticweb.org/pddl#equal"/>
                        <pddl:parameter>
                            <rdf:Description>
